{
  "scaling": {
    "scaling": {
      "factor": {
        "90%": {
          "$type": "number",
          "$value": "0.9"
        },
        "95%": {
          "$type": "number",
          "$value": "0.95"
        },
        "100%": {
          "$type": "number",
          "$value": "1"
        },
        "105%": {
          "$type": "number",
          "$value": "1.05"
        },
        "110%": {
          "$type": "number",
          "$value": "1.1"
        }
      }
    }
  },
  "spacing": {
    "spacing": {
      "baseUnit": {
        "$type": "spacing",
        "$value": "0.25rem * {spacing.scalingFactor}"
      },
      "unit": {
        "1": {
          "$type": "spacing",
          "$value": "1 * {spacing.baseUnit}"
        },
        "2": {
          "$type": "spacing",
          "$value": "2 * {spacing.baseUnit}"
        },
        "3": {
          "$type": "spacing",
          "$value": "3 * {spacing.baseUnit}"
        },
        "4": {
          "$type": "spacing",
          "$value": "4 * {spacing.baseUnit}"
        },
        "5": {
          "$type": "spacing",
          "$value": "5 * {spacing.baseUnit}"
        },
        "6": {
          "$type": "spacing",
          "$value": "6 * {spacing.baseUnit}"
        },
        "7": {
          "$type": "spacing",
          "$value": "7 * {spacing.baseUnit}"
        },
        "8": {
          "$type": "spacing",
          "$value": "8 * {spacing.baseUnit}"
        },
        "9": {
          "$type": "spacing",
          "$value": "9 * {spacing.baseUnit}"
        },
        "10": {
          "$type": "spacing",
          "$value": "10 * {spacing.baseUnit}"
        },
        "11": {
          "$type": "spacing",
          "$value": "11 * {spacing.baseUnit}"
        },
        "12": {
          "$type": "spacing",
          "$value": "12 * {spacing.baseUnit}"
        },
        "16": {
          "$type": "spacing",
          "$value": "16 * {spacing.baseUnit}"
        },
        "20": {
          "$type": "spacing",
          "$value": "20 * {spacing.baseUnit}"
        },
        "24": {
          "$type": "spacing",
          "$value": "24 * {spacing.baseUnit}"
        },
        "32": {
          "$type": "spacing",
          "$value": "32 * {spacing.baseUnit}"
        },
        "36": {
          "$type": "spacing",
          "$value": "36 * {spacing.baseUnit}"
        },
        "40": {
          "$type": "spacing",
          "$value": "40 * {spacing.baseUnit}"
        },
        "half": {
          "$type": "spacing",
          "$value": "0.5 * {spacing.baseUnit}"
        }
      },
      "scalingFactor": {
        "$type": "number",
        "$value": "{scaling.factor.90%}"
      },
      "gap": {
        "xs": {
          "$type": "spacing",
          "$value": "{spacing.unit.1}"
        },
        "sm": {
          "$type": "spacing",
          "$value": "{spacing.unit.2}"
        },
        "md": {
          "$type": "spacing",
          "$value": "{spacing.unit.4}"
        },
        "lg": {
          "$type": "spacing",
          "$value": "{spacing.unit.6}"
        },
        "xl": {
          "$type": "spacing",
          "$value": "{spacing.unit.8}"
        }
      }
    }
  },
  "typography": {
    "typography": {
      "fontWeight": {
        "medium": {
          "$type": "fontWeights",
          "$value": "500",
          "$description": "Medium font weight"
        },
        "regular": {
          "$type": "fontWeights",
          "$value": "400"
        },
        "light": {
          "$type": "fontWeights",
          "$value": "300"
        },
        "bold": {
          "$type": "fontWeights",
          "$value": "680"
        }
      },
      "fontFamily": {
        "body": {
          "$type": "fontFamilies",
          "$value": "Inter, Arial, sans-serif",
          "$description": "The base font family"
        },
        "heading": {
          "$type": "fontFamilies",
          "$value": "{typography.fontFamily.body}",
          "$description": "The font family for all headings"
        },
        "mono": {
          "$type": "fontFamilies",
          "$value": "SFMono-Regular, monospace",
          "$description": "The font family for monospaced text"
        }
      },
      "letterSpacing": {
        "tighter": {
          "$type": "letterSpacing",
          "$value": "-0.05em"
        },
        "tight": {
          "$type": "letterSpacing",
          "$value": "-0.025em"
        },
        "regular": {
          "$type": "letterSpacing",
          "$value": "0em"
        },
        "wide": {
          "$type": "letterSpacing",
          "$value": "0.025em"
        },
        "wider": {
          "$type": "letterSpacing",
          "$value": "0.05em"
        },
        "widest": {
          "$type": "letterSpacing",
          "$value": "0.1em"
        }
      },
      "lineHeight": {
        "shorter": {
          "$type": "lineHeights",
          "$value": "1.25"
        },
        "short": {
          "$type": "lineHeights",
          "$value": "1.375"
        },
        "moderate": {
          "$type": "lineHeights",
          "$value": "1.5"
        },
        "tall": {
          "$type": "lineHeights",
          "$value": "1.625"
        },
        "taller": {
          "$type": "lineHeights",
          "$value": "2"
        }
      },
      "fontSize": {
        "2xs": {
          "$type": "fontSizes",
          "$value": "0.625rem"
        },
        "xs": {
          "$type": "fontSizes",
          "$value": "0.75rem"
        },
        "sm": {
          "$type": "fontSizes",
          "$value": "0.875rem"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "1rem",
          "$description": "Base font size: 1rem, 16px"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "1.125rem"
        },
        "xl": {
          "$type": "fontSizes",
          "$value": "1.25rem"
        },
        "2xl": {
          "$type": "fontSizes",
          "$value": "1.5rem"
        }
      }
    }
  },
  "colors": {
    "colors": {
      "divider": {
        "$type": "color",
        "$value": "#E1E0E6"
      },
      "background": {
        "default": {
          "$type": "color",
          "$value": "#F7F6F8"
        },
        "panel": {
          "$type": "color",
          "$value": "#bcbcbc"
        }
      },
      "text": {
        "primary": {
          "$type": "color",
          "$value": "{colors.common.black}"
        },
        "secondary": {
          "$type": "color",
          "$value": "#8A8896"
        }
      },
      "semantic": {
        "danger": {
          "$type": "color",
          "$value": "#cc2a1e"
        },
        "success": {
          "$type": "color",
          "$value": "#04ad45"
        }
      },
      "common": {
        "white": {
          "$type": "color",
          "$value": "#FFFFFF"
        },
        "black": {
          "$type": "color",
          "$value": "#000000"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "scaling",
      "spacing",
      "typography",
      "colors"
    ]
  }
}