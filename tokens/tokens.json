{
  "scaling": {
    "factor": {
      "90%": {
        "$type": "number",
        "$value": "0.9"
      },
      "95%": {
        "$type": "number",
        "$value": "0.95"
      },
      "100%": {
        "$type": "number",
        "$value": "1"
      },
      "105%": {
        "$type": "number",
        "$value": "1.05"
      },
      "110%": {
        "$type": "number",
        "$value": "1.1"
      }
    }
  },
  "spacing": {
    "baseUnit": {
      "$type": "spacing",
      "$value": "0.25rem * {scalingFactor}"
    },
    "unit": {
      "1": {
        "$type": "spacing",
        "$value": "1 * {baseUnit}"
      },
      "2": {
        "$type": "spacing",
        "$value": "2 * {baseUnit}"
      },
      "3": {
        "$type": "spacing",
        "$value": "3 * {baseUnit}"
      },
      "4": {
        "$type": "spacing",
        "$value": "4 * {baseUnit}"
      },
      "5": {
        "$type": "spacing",
        "$value": "5 * {baseUnit}"
      },
      "6": {
        "$type": "spacing",
        "$value": "6 * {baseUnit}"
      },
      "7": {
        "$type": "spacing",
        "$value": "7 * {baseUnit}"
      },
      "8": {
        "$type": "spacing",
        "$value": "8 * {baseUnit}"
      },
      "9": {
        "$type": "spacing",
        "$value": "9 * {baseUnit}"
      },
      "10": {
        "$type": "spacing",
        "$value": "10 * {baseUnit}"
      },
      "11": {
        "$type": "spacing",
        "$value": "11 * {baseUnit}"
      },
      "12": {
        "$type": "spacing",
        "$value": "12 * {baseUnit}"
      },
      "16": {
        "$type": "spacing",
        "$value": "16 * {baseUnit}"
      },
      "20": {
        "$type": "spacing",
        "$value": "20 * {baseUnit}"
      },
      "24": {
        "$type": "spacing",
        "$value": "24 * {baseUnit}"
      },
      "32": {
        "$type": "spacing",
        "$value": "32 * {baseUnit}"
      },
      "36": {
        "$type": "spacing",
        "$value": "36 * {baseUnit}"
      },
      "40": {
        "$type": "spacing",
        "$value": "40 * {baseUnit}"
      },
      "half": {
        "$type": "spacing",
        "$value": "0.5 * {baseUnit}"
      }
    },
    "scalingFactor": {
      "$type": "number",
      "$value": "{factor.100%}"
    },
    "gap": {
      "xs": {
        "$type": "spacing",
        "$value": "{unit.1}"
      },
      "sm": {
        "$type": "spacing",
        "$value": "{unit.2}"
      },
      "md": {
        "$type": "spacing",
        "$value": "{unit.4}"
      },
      "lg": {
        "$type": "spacing",
        "$value": "{unit.6}"
      },
      "xl": {
        "$type": "spacing",
        "$value": "{unit.8}"
      }
    }
  },
  "typography": {
    "fontWeight": {
      "medium": {
        "$type": "fontWeights",
        "$value": "500",
        "$description": "Medium font weight"
      },
      "regular": {
        "$type": "fontWeights",
        "$value": "400"
      },
      "light": {
        "$type": "fontWeights",
        "$value": "300"
      },
      "bold": {
        "$type": "fontWeights",
        "$value": "680"
      }
    },
    "fontFamily": {
      "body": {
        "$type": "fontFamilies",
        "$value": "Inter, Arial, sans-serif",
        "$description": "The base font family"
      },
      "heading": {
        "$type": "fontFamilies",
        "$value": "{fontFamily.body}",
        "$description": "The font family for all headings"
      },
      "mono": {
        "$type": "fontFamilies",
        "$value": "SFMono-Regular, monospace",
        "$description": "The font family for monospaced text"
      }
    },
    "letterSpacing": {
      "tighter": {
        "$type": "letterSpacing",
        "$value": "-0.05em"
      },
      "tight": {
        "$type": "letterSpacing",
        "$value": "-0.025em"
      },
      "regular": {
        "$type": "letterSpacing",
        "$value": "0em"
      },
      "wide": {
        "$type": "letterSpacing",
        "$value": "0.025em"
      },
      "wider": {
        "$type": "letterSpacing",
        "$value": "0.05em"
      },
      "widest": {
        "$type": "letterSpacing",
        "$value": "0.1em"
      }
    },
    "lineHeight": {
      "shorter": {
        "$type": "lineHeights",
        "$value": "1.25"
      },
      "short": {
        "$type": "lineHeights",
        "$value": "1.375"
      },
      "moderate": {
        "$type": "lineHeights",
        "$value": "1.5"
      },
      "tall": {
        "$type": "lineHeights",
        "$value": "1.625"
      },
      "taller": {
        "$type": "lineHeights",
        "$value": "2"
      }
    },
    "fontSize": {
      "2xs": {
        "$type": "fontSizes",
        "$value": "0.625rem"
      },
      "xs": {
        "$type": "fontSizes",
        "$value": "0.75rem"
      },
      "sm": {
        "$type": "fontSizes",
        "$value": "0.875rem"
      },
      "md": {
        "$type": "fontSizes",
        "$value": "1rem",
        "$description": "Base font size: 1rem, 16px"
      },
      "lg": {
        "$type": "fontSizes",
        "$value": "1.125rem"
      },
      "xl": {
        "$type": "fontSizes",
        "$value": "1.25rem"
      },
      "2xl": {
        "$type": "fontSizes",
        "$value": "1.5rem"
      }
    }
  },
  "colors": {
    "divider": {
      "$type": "color",
      "$value": "#E1E0E6"
    },
    "background": {
      "default": {
        "$type": "color",
        "$value": "#F7F6F8"
      },
      "panel": {
        "$type": "color",
        "$value": "{common.white}"
      }
    },
    "text": {
      "primary": {
        "$type": "color",
        "$value": "{common.black}"
      },
      "secondary": {
        "$type": "color",
        "$value": "#8A8896"
      }
    },
    "semantic": {
      "danger": {
        "$type": "color",
        "$value": "#cc2a1e"
      },
      "success": {
        "$type": "color",
        "$value": "#04ad45"
      }
    },
    "common": {
      "white": {
        "$type": "color",
        "$value": "#FFFFFF"
      },
      "black": {
        "$type": "color",
        "$value": "#000000"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "scaling",
      "spacing",
      "typography",
      "colors"
    ]
  }
}